	outline:0 none;
}
.form-edit tr:first-child th,.form-edit .table-condensed td{
	padding-top:0px;
	padding-left:0px;
	padding-bottom:0px;
}

.input-container{
	min-width:220.5px;
}

.container-fluid{
	//border-color: rgb(179, 180, 182) rgb(179, 180, 182) rgb(205, 205, 205);
	//border-style: solid;
	//border-width:1px;
	padding:10px 5px;
	margin-top:-11px;
}

.bar {
    background: linear-gradient(to bottom, rgb(250, 250, 250) 0%, rgb(239, 239, 239) 100%) repeat scroll 0 0 transparent;
    border-color: rgb(179, 180, 182) rgb(179, 180, 182) rgb(205, 205, 205);
    border-style: solid;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
    border-width: 1px;
    color: rgb(0, 0, 0);
    font-size: 14px;
    font-weight: bold;
    height: 30px;
    padding: 16px 15px 0;
    text-shadow: 1px 1px 1px rgb(255, 255, 255);
    width: 98%;
}

.bar > span{
	padding-left:20px;
}

.header{
	width:100%;
}

.nav{
	width:100%;
}

.nav > li{
}

.search-block{
	margin: 10px;
    width: 100%;
	padding-left: 7px;
}

.table{
	position:relative;
}

.group-table > tbody{
	position:absolute;
	border-color: rgb(221, 221, 221);
    border-image: none;
    border-radius: 4px 4px 4px 4px;
    border-style: solid solid solid solid;
    border-width: 1px 1px 1px 1px;
	height:464px;
	width:99.5%;
	overflow-y:scroll;
}

.auth-table > tbody{
	position:absolute;
	border-color: rgb(221, 221, 221);
    border-image: none;
    border-style: solid solid solid solid;
    border-width: 1px 1px 1px 1px;
	height:437px;
	width:99.5%;
	overflow-y:scroll;
}

.user-table > tbody > tr > td,.user-table > thead > tr > th{
	position:relative;
	width:23%;
}

.user-table > tbody > tr > td:first-child,.user-table > thead > tr > th:first-child{
	position:relative;
	width:14%;
}

.group-table,.group-table > thead > tr > th{
	border-width:0px;
}

.group-table > tbody > tr > td,.group-table  > thead > tr > th{
	position:relative;
	width:1000px;
}

.group-table > tbody > tr > td:first-child,.group-table > thead > tr > th:first-child{
	position:relative;
	width:20px;
}

.group-table > tbody > tr > td > a{
	//background-color:red;
	padding:10px 100px 10px 10px;
	margin-left:0px;
	text-decoration:none;
	color:black;
}

.group-table > tbody > tr > td > a:hover.group-table > tbody > tr > td > a:focus{
	text-decoration:none;
}

.group-table > tbody > tr > td:hover{
	cursor:pointer;
}

.auth-table,.auth-table > thead > tr > th{
	border-width:0px;
}

.auth-table > thead > tr > th:first-child{
	position:relative;
	width:25px;
}

.auth-table > thead > tr > th:last-child{
	position:relative;
	width:1000px;
}

.auth-table  > thead > tr > th{
	position:relative;
	width:230px;
}

.auth-table > tbody > tr > td:first-child{
	position:relative;
	max-width:20px;
}

.auth-table > tbody > tr > td:last-child{
	position:relative;
	width:800px;
}

.auth-table > tbody > tr > td{
	position:relative;
	width:200px;
}

.auth-block,.author-block{
	border-color: rgb(221, 221, 221);
    border-image: none;
    border-style: solid solid solid solid;
    border-width: 1px 1px 1px 1px;
	margin-top:37px;
	height:524px;
	background-color:#fff;
}

.url-btns{
	margin-left: 16px;
}

.author-block{
}

h1, h2, h3, h4, h5, h6{
	line-height: 10px;
}

.clearfix:after{
	content:".";
	display:block;
	height:0;
	visibility:hidden;
	clear:both;
}

.left-float{
	float:left;
}

.tree {
	position:absolute;
	overflow-y:scroll;
    //min-height:20px;
	//max-height:100px;
	height:542px;
    padding:19px;
	width:62%;
    margin-bottom:20px;
    //background-color:#fbfbfb;
    //border:1px solid #999;
    //-webkit-border-radius:4px;
    //-moz-border-radius:4px;
    //border-radius:4px;
    //-webkit-box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.05);
    //-moz-box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.05);
    //box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.05)
}

.tree ul {
	margin-top:3px;
}

.tree li {
    list-style-type:none;
    margin:0;
    padding:10px 5px 0 5px;
    position:relative
}

.tree li::before, .tree li::after {
    content:'';
    left:-20px;
    position:absolute;
    right:auto
}
.tree li::before {
    border-left:1px solid #999;
    bottom:50px;
    height:100%;
    top:-4px;
    width:1px
}
.tree li::after {
    border-top:1px solid #999;
    height:20px;
    top:25px;
    width:25px
}
.tree li span {
    -moz-border-radius:5px;
    -webkit-border-radius:5px;
    border:1px solid #999;
    border-radius:5px;
    display:inline-block;
    padding:3px 8px;
    text-decoration:none;
}

.tree li span:hover {
	//background-color:black;
}

.tree li.parent_li>span {
    cursor:pointer
}
.tree>ul>li::before, .tree>ul>li::after {
    border:0
}
.tree li:last-child::before {
    height:30px
}
.tree li.parent_li>span:hover, .tree li.parent_li>span:hover+ul li span {
   }

.parent_li span{
	margin-top:4px;
	min-width:50px;
}

.parent_li ul{
	margin-left: 52px;
}

